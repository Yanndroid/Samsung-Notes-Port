<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/setting_palette_indicator_selected_color" />
        </shape>
    </item>
    <item>
        <inset android:inset="1.0dip">
            <shape android:shape="oval">
                <solid android:color="@color/setting_palette_indicator_unselected_color" />
            </shape>
        </inset>
    </item>
</selector>