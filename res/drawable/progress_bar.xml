<?xml version="1.0" encoding="utf-8"?>
<layer-list android:autoMirrored="true"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background" android:height="2.0dip">
        <shape>
            <solid android:color="@color/primary_progress_background" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress" android:height="2.0dip">
        <clip>
            <shape>
                <solid android:color="@color/primary_progress_background" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" android:height="2.0dip">
        <clip>
            <shape>
                <solid android:color="@color/primary" />
            </shape>
        </clip>
    </item>
</layer-list>