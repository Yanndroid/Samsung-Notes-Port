<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@id/progress_track_color">
        <shape android:shape="rectangle">
            <solid android:color="#00000000" />
            <corners android:radius="@dimen/setting_slider_progress_bg_radius" />
        </shape>
    </item>
    <item android:id="@id/progress_track_border" android:top="0.0dip" android:bottom="0.0dip">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/setting_slider_progress_bg_radius" />
            <stroke android:width="1.0dip" android:color="#00000000" />
        </shape>
    </item>
</layer-list>